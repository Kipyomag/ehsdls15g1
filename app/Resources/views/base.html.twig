<html lang='fr'>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0">

        <title>{% block title %}{% endblock %} | EHS</title>

        {% block stylesheets %}
            <!-- Bootstrap core CSS -->
            <link rel="stylesheet" href="{{ asset('public/css/bootstrap.min.css') }}" type="text/css" media="all" />
            <!-- Custom Google Web Font -->
            <link rel="stylesheet" href="{{ asset('public/font-awesome/css/font-awesome.min.css') }}" type="text/css" media="all" />
            <link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic' rel='stylesheet' type='text/css'>
            <link href='http://fonts.googleapis.com/css?family=Arvo:400,700' rel='stylesheet' type='text/css'>

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
            <!-- Custom CSS-->
        <link rel="stylesheet" href="{{ asset('public/css/general.css') }}" type="text/css" media="all" />

            <!-- Owl-Carousel -->
        <link rel="stylesheet" href="{{ asset('public/css/custom.css') }}" type="text/css" media="all" />
        <link rel="stylesheet" href="{{ asset('public/css/owl.carousel.css') }}" type="text/css" media="all" />
