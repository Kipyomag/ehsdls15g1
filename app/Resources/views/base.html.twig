        <link rel="stylesheet" href="{{ asset('public/css/magnific-popup.css') }}" type="text/css" media="all" />


    </head>
        {% endblock %}

<body id="home">
    <!-- Preloader -->
    <div id="preloader">
        <div id="status"></div>
    </div>

    <!-- NavBar-->
    <nav class="navbar-default" role="navigation">

        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="{{ path('articles_index') }}">EHS</a>
            </div>

            <div class="collapse navbar-collapse navbar-right navbar-ex1-collapse">
                <ul class="nav navbar-nav">
                    {% if app.user %}
                    {% if is_granted('ROLE_ADMIN') %}

                    <li class="menuItem"><a href="{{ path('admin_index') }}">Administration</a></li>
                    {% endif %}
                    <li class="menuItem"><a href="{{ path('users_show_profil', { 'id': app.user.id }) }}">Profil</a></li>
                        <li class="menuItem"><a href="{{ path('users_show_profil', { 'id': app.user.id }) }}">Profil</a></li>
                        <li class="menuItem"><a href="{{ path('articles_index') }}">Articles</a></li>
                        <li class="menuItem"><a href="{{ path('presentation_index') }}">L'association</a></li>
                        <li class="menuItem"><a href="#">Nos actions</a></li>
                        <li class="menuItem"><a href="#">Agir & soutenirs</a></li>
                        <li class="menuItem"><a href="#">Presse</a></li>
                        <li class="menuItem"><a href="{{ path('image_index') }}">Médias</a></li>
                        <li class="menuItem"><a href="{{ path('archive_index') }}">Archives</a></li>
                        <li class="menuItem"><a href="{{ path('contact_index') }}">Contact</a></li>
                        <li class="menuItem"><a href="{{ path('users_logout') }}">Déconnexion</a></li>
                    {% else %}

                        <li class="menuItem"><a href="{{ path('articles_index') }}">Accueil</a></li>
                        <li class="menuItem"><a href="{{ path('presentation_index') }}">L'association</a></li>
                        <li class="menuItem"><a href="#">Nos actions</a></li>
                        <li class="menuItem"><a href="#">Agir & soutenirs</a></li>
                        <li class="menuItem"><a href="#">Presse</a></li>
                        <li class="menuItem"><a href="{{ path('image_index') }}">Médias</a></li>
                        <li class="menuItem"><a href="{{ path('contact_new') }}">Contact</a></li>
                        <li class="menuItem"><a href="{{ path('users_new') }}">Inscription</a></li>
                        <li class="menuItem"><a href="{{ path('users_login') }}">Connexion</a></li>
                    {% endif %}
                </ul>
            </div>
        </div>
    </nav>

    <!-- Content -->
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                    {% block body %}
                    {% endblock %}
            </div>
        </div>
    </div>

    <!-- footer -->
    <footer class="footer">
        <div class="footer-top">
            <div class="container">
                <div class="row">
                    <div class="footer-col col-md-4">
                        <h5>Location</h5>
                        <p>Nantes</p>
                        <h5>Notre Siège</h5>
                        <p>6 avenue de la Devinière <br>44000 Nantes</p>
                    </div>
                    <div class="footer-col col-md-4">
                        <h5>Share with Love</h5>
                        <ul class="footer-share">
                            <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="https://twitter.com/kamal_chaneman"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="https://www.linkedin.com/in/kamalchaneman"><i class="fa fa-linkedin"></i></a></li>
                            <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                        </ul>
                    </div>
                    <div class="footer-col col-md-4">
                        <h5>About EHS</h5>
                        <p>Agissons ensemble.</p>
                    </div>
                </div>
            </div>
        </div><!-- footer top -->
        <div class="footer-bottom">
            <div class="container">
                <div class="col-md-12">

                        <p>IMIE - 2016 - dl15</p>

                </div>
            </div>
        </div>
    </footer>
